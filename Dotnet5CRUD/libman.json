{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "bootstrap-icons@1.8.1",
      "destination": "wwwroot/bootstrap-icons/",
      "files": [
        "font/fonts/bootstrap-icons.woff",
        "font/fonts/bootstrap-icons.woff2",
        "font/bootstrap-icons.min.css"
      ]
    },
    {
      "library": "jquery-validation-unobtrusive@3.2.12",
      "destination": "wwwroot/assets/css/bootstrap/js/jquery-validation-unobtrusive/"
    },
    {
      "library": "bootstrap-datepicker@1.9.0",
      "destination": "wwwroot/assets/Lib/bootstrap-datepicker/",
      "files": [
        "js/bootstrap-datepicker.min.js",
        "css/bootstrap-datepicker.min.css"
      ]
    },
    {
      "library": "bootstrap@5.1.3",
      "destination": "wwwroot/assets/css/bootstrap/bootstrap/"
    }
  ]
}